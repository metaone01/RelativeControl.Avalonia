<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:HeadlessTest.RelativeControl">
    <Design.PreviewWith>
        <controls:Container />
    </Design.PreviewWith>

    <Style Selector="controls|Container">
        <!-- Set Defaults -->
        <Setter Property="Template">
            <ControlTemplate>
                <TextBlock Name="PART_Text" Text="Templated Control" />
            </ControlTemplate>
        </Setter>
    </Style>
</Styles>
